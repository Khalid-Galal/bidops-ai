{
  "name": "bidops-ai-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for BidOps AI using Playwright",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:admin": "playwright test admin-e2e.spec.ts",
    "test:admin:headed": "playwright test admin-e2e.spec.ts --headed",
    "test:admin:debug": "playwright test admin-e2e.spec.ts --debug",
    "test:admin:chrome": "playwright test admin-e2e.spec.ts --project=chromium",
    "generate:files": "node test-files/generate-test-files.js",
    "clean:screenshots": "rimraf tests/screenshots/*.png",
    "pretest": "node test-files/generate-test-files.js"
  },
  "keywords": ["e2e", "playwright", "testing", "bidops"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.3.0"
  }
}
